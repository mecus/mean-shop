{
  "name": "shop-app",
  "version": "1.0.0",
  "description": "Online Shopping app",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json -w",
    "serve": "lite-server -c=bs-config.json",
    "gulp:clean": "gulp clean && gulp clean:public",
    "gulp": "gulp",
    "build:dev": "gulp clean && npm run build && gulp build:dev",
    "build:prod": "gulp clean:public && npm run build && gulp build:prod ",
    "start": "concurrently \"gulp sass:watch\" \"npm run build:watch\" \"nodemon server.js\""
  },
